@model Person
<div class="row">
    <div class="col-sm text-center">
        @Model.id
    </div>
    <div class="col-sm text-center">
        @Model.Name
    </div>
    <div class="col-sm text-center">
        @Model.City.CityName
    </div>
    <div class="col-sm text-center">
        @Model.PhoneNumber
    </div>

</div>