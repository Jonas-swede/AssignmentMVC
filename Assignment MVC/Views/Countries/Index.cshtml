@model IEnumerable<Country>
<h1>Countries</h1>
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CountryName)
            </th>
         
        </tr>
    </thead>

    <tbody>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.CountryName)
            </td>
            <td>
                <a asp-action="EditCountry" asp-route-CountryId="@item.CountryId">Edit country</a><br />
                <a asp-action="RemoveCountry" asp-route-CountryId="@item.CountryId">Delete country</a>
            </td>
        </tr>
    }
    </tbody>
</table>